# Read
data <- read_csv("group16.csv")
colnames(data) <- c("Gender", "Age", "Study_Program", "Diet", "Eat_Out_Per_Week", "Alcohol_Per_Month")
# Clean diet type column
data$Diet <- gsub("\\s*\\(.*\\)", "", data$Diet)
# Clean Study Program
#print(as.list(unique(data$Study_Program)))
cleaned_data <- data
#Degree
cleaned_data$Degree = ""
cleaned_data <- cleaned_data %>% mutate(Degree = case_when(
grepl("BSc", Study_Program, ignore.case=TRUE) ~ "BSc",
grepl("Bachelor", Study_Program, ignore.case=TRUE) ~ "BSc",
grepl("MSc", Study_Program, ignore.case=TRUE) ~ "MSc",
grepl("Master", Study_Program, ignore.case=TRUE) ~ "MSc",
grepl("Data Science", Study_Program, ignore.case=TRUE) ~ "MSc",
.default = NA))
#Program
cleaned_data <- cleaned_data %>% mutate(Study_Program = case_when(
grepl("Data Science", Study_Program, ignore.case=TRUE) ~ "Data Science",
grepl("DataScience", Study_Program, ignore.case=TRUE) ~ "Data Science",
grepl("Mathematic", Study_Program, ignore.case=TRUE) ~ "Mathematics",
grepl("Informat", Study_Program, ignore.case=TRUE) ~ "Informatics",
.default = NA))
# Set "no" and "never" answers for numerical data to 0
cleaned_data$Alcohol_Per_Month[cleaned_data$Alcohol_Per_Month == "never"] = 0
cleaned_data$Eat_Out_Per_Week[cleaned_data$Eat_Out_Per_Week == "no"] = 0
cleaned_data <- cleaned_data %>%
mutate(Alcohol_Per_Month = as.numeric(Alcohol_Per_Month), Eat_Out_Per_Week = as.numeric(Eat_Out_Per_Week))
# Create columns Alcohol_Per_Week and Eat_Out_Per_Month
cleaned_data <- cleaned_data %>%
mutate(Eat_Out_Per_Month = Eat_Out_Per_Week * 52 / 12, Alcohol_Per_Week = Alcohol_Per_Month * 12 / 52)
# Create condensed diet column where vegetarians and vegans are combined (because of low population of vegan group)
cleaned_data <- cleaned_data %>%
mutate(Diet_Condensed = fct_collapse(Diet, Vegetarian = c("Vegetarian", "Vegan")))
# Create column differentiating between meat eating and non meat eating people
cleaned_data <- cleaned_data %>%
mutate(Eats_Meat = Diet_Condensed != "Vegetarian")
# Dataframe containing a subset consisitng of only males and females
male_female_data = cleaned_data %>% filter(Gender %in% c("male", "female"))
gender_plot <- ggplot(cleaned_data, aes(x = Gender, fill = Gender)) +
geom_bar(fill="lightgreen", color="black") +
labs(title = "Gender Distribution", x = "Gender", y = "Count") +
scale_y_continuous(n.breaks = 10)
age_plot <- ggplot(cleaned_data, aes(x = as.numeric(Age))) +
geom_histogram(binwidth = 5, fill = "lightgreen", color = "black") +
labs(title = "Age Distribution", x = "Age", y = "Count") +
scale_x_continuous(n.breaks = 10) +
scale_y_continuous(n.breaks = 10) +
theme_minimal()
degree_plot <- ggplot(cleaned_data, aes(x = Degree)) +
geom_bar(fill = "lightgreen", color = "black") +
labs(title = "Distribution of Degree Types", x = "Degree Type", y = "Count") +
scale_y_continuous(n.breaks = 10) +
theme_minimal()
study_plot <- ggplot(cleaned_data, aes(x = Study_Program)) +
geom_bar(fill = "lightgreen", color = "black") +
labs(title = "Distribution of Study Types", x = "Study Type", y = "Count") +
scale_y_continuous(n.breaks = 10) +
theme_minimal()
grid.arrange(gender_plot, age_plot, degree_plot, study_plot, ncol = 2)
proportions <- cleaned_data %>%
count(Gender, Eats_Meat) %>%
group_by(Gender) %>%
mutate(Proportion = n / sum(n))
ggplot(proportions, aes(fill = factor(Eats_Meat), y = Proportion, x = Gender)) +
geom_bar(position="fill", stat="identity") +
scale_fill_discrete(labels = c("FALSE" = "Non Meat Diet", "TRUE" = "Meat Diet")) +
labs(fill = "Diet Type",
